<%= form_with model: @user do |form| %>
  Username: <%= form.text_field :username, value: "username" %>
  Email: <%= form.email_field :email, placeholder: "example@example.com" %>
  Password: <%= form.password_field :password %>
  <%= form.submit %>
<% end %>